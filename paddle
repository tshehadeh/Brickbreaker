class Paddle{
  float x;
  float y;
  float w;
  float h;
  float speed = 5;

  
  Paddle(){
    
    x = 350;
    y = 550;
    w=100;
    h = 10;
    
  }
  void checkEdges(){
    if(paddle.x+50>700){
    paddle.x = 650;
  }
  if(paddle.x-50<0){
    paddle.x = 50;
  }
  
    
  }
  void show(){
    
    rect(x,y,w,h);
    
  }
  
  
  
}
